{
  "name": "react-step-by-step",
  "version": "1.0.0",
  "description": "react-step-by-step 是一个从零开始学习 react 的教程。",
  "scripts": {
    "clean": "rimraf ./dist npm-debug.log*",
    "lint": "eslint -c config/.eslintrc.js --ext .js,.jsx app/",
    "dev": "webpack-dev-server --config config/webpack.config.dev.js --color",
    "prod": "webpack --config config/webpack.config.prod.js --color",
    "start": "if-env NODE_ENV=production && npm run prod || npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zp1024/react-step-by-step"
  },
  "keywords": [
    "react",
    "npm",
    "webpack",
    "es6",
    "eslint",
    "babel"
  ],
  "author": "Zhang Peng",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.18.1",
    "prop-types": "^15.6.0",
    "react": "15.x",
    "react-dom": "15.x"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "^8.0.0",
    "babel-loader": "7.x",
    "babel-plugin-syntax-dynamic-import": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "css-loader": "0.x",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "2.x",
    "file-loader": "0.x",
    "html-webpack-plugin": "2.x",
    "if-env": "1.x",
    "image-webpack-loader": "3.x",
    "open-browser-webpack-plugin": "0.x",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "2.x",
    "style-loader": "0.x",
    "url-loader": "^0.5.9",
    "webpack": "2.x",
    "webpack-dev-server": "2.x",
    "webpack-merge": "4.x"
  }
}
